package calculator;



public class StringCalculator {
	
	public static int add(String numbers) {
		int result=0;
		if(numbers.length()==0)
			return result;
		String[] splitted_string=numbers.split(",");

		for(int i=0;i<splitted_string.length;i++) {
			result+=Integer.parseInt(splitted_string[i]);
		}
		return result;
		
	}
}
